@model IEnumerable<WorkOrderEMS.Models.NewAdminModel.GWCQUestionModel>

@{
    //Layout = "~/Views/Shared/_NewAdminLayout.cshtml";
    Layout = null;
    int count = 0;
}

<div class="col-sm-12 col-xs-12 col-md-12 glowbuttonActiveDeactive"> Self Assessment</div>
<div class="table-responsive">
    <button id="back" class="btn btn-info pull-right">Back</button>

    <table class="table">
        <tbody>
            @foreach (var item in Model)
            {
                if (item.QuestionType == "G30" && count == 0)
                {

                    <tr class="table-primary">
                        <td>Get It</td>


                        <td>
                            self eval
                        </td>
                    </tr>

                    count = count + 1;
                }
                else if (item.QuestionType == "W30" && count == 1)
                {
                    <tr class="table-primary">
                        <td>Want It</td>


                        <td>
                            self eval
                        </td>
                    </tr>
                    count = count + 1;

                }
                else if (item.QuestionType == "C30" && count == 2)
                {
                    <tr class="table-primary">
                        <td>Capacity to do It</td>


                        <td>
                            self eval
                        </td>
                    </tr>
                    count = count + 1;

                }

                <tr class="table-light">
                    <td>
                        @Html.DisplayFor(modelItem => item.Question)
                    </td>
                    <td>

                        <input type="radio" value="Yes" name="Yes" /> Yes

                        <input type="radio" value="No" name="No" /> No
                    </td>
                </tr>
            }

            <tr align="center">
                <td>

                    <button id="draft" class="btn btn-secondary">Draft</button>

                    <button id="submit" class="btn btn-success">Submit</button>
                </td>
            </tr>
        </tbody>
    </table>
</div>
<script>
    $("#back").on('click', function () {
        $("#gridArea").show();
        $('#profileArea').hide();
        $("#List306090").jsGrid("loadData");
    });</script>
