<script>
    $("document").ready(function () {
        var base_url = window.location.origin;
        enableDisableBtn();
        $("td.ans input[type='radio']").on("click", function () {
            enableDisableBtn();
        })
        $("#uploadDisputeAttachment").change(function () {

            var allowedExtensions = /(\.pdf|\.doc|\.docx|\.txt)$/i;
            var size = this.files[0].size;
            $("#uploadDisputeAttachmentLabel").html(this.files[0].name);
            if (!allowedExtensions.exec($(this).val().toLowerCase())) {
                alert("Invalid file type");
                $(this).val("");
                return false;
            }
            else if (size >= 4194304) {
                alert("Max file size 4 mb.");
                $(this).val("");
                return false;
            }
        })
    });
