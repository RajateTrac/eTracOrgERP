@model WorkOrderEMS.Models.eFleetDriverModel
@{
    ViewBag.Title = "Insurance/License List";
    Layout = "~/Views/Shared/_ManagerLayout.cshtml";

    long locationId = 0;
    long userType = 0;
    WorkOrderEMS.Models.eTracLoginModel ObjLogin = (WorkOrderEMS.Models.eTracLoginModel)Session["eTrac"];

    if (ObjLogin != null)
    {
        locationId = ObjLogin.LocationID;
        userType = ObjLogin.UserRoleId;
    }
}
@section scripts{
    <script type="text/javascript">
    var $_locationId = '@locationId';
    var $_userTypeId = '@userType';
        var $_VendorID = '@ViewBag.VendorId';
        var $_VendorStatus = '@ViewBag.VendorStatus';
    </script>
    <script src="~/GridListing/JqGridScript/CompanyList/ListInsurance.js"></script>
    <script src="~/GridListing/JqGridScript/CompanyList/ListLicense.js"></script>
    <script type="text/javascript">
    $(window).bind('resize', function () {
        var width = $('#jqGrid_container,#jqGrid_containerLicense').width();
        $('#tbl_LicenseAndInsuranceList,#tbl_LicenseList').setGridWidth(width);
    });
    </script>
}

<!--For Insurance-->
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div id="jqGrid_container">
        <table id="tbl_LicenseAndInsuranceList" border="0" cellpadding="0" cellspacing="0"></table>
        <div id="divLicenseAndInsuranceListPager"></div>
    </div>
</div>
<!--End Insurance-->
<br /><br />
<!--For License-->
<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
    <div id="jqGrid_containerLicense" style="margin-top: 35px;">
        <table id="tbl_LicenseList" border="0" cellpadding="0" cellspacing="0"></table>
        <div id="divLicenseListPager"></div>
    </div>
</div>
<!--End Insurance-->
