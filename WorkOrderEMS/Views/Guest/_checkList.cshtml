            $("#unCheckedropform").css('display', 'none');
        }
        if (FormStatusprfcaf === "true") {
            $("#Checkcafform").css('display', 'block');
            $("#unCheckedcafform").css('display', 'none');
        }
        if (FormStatussif === "true") {
            $("#Checksifform").css('display', 'block');
            $("#unCheckedsifform").css('display', 'none');
            $("#checkSif").css('display', 'flex');
        }
        if (FormStatusprf === "true") {
            $("#Checkprfform").css('display', 'block');
            $("#unCheckedprfform").css('display', 'none');
        }
        if (FormStatusff === "true") {
            $("#Checkffform").css('display', 'block');
            $("#unCheckedffform").css('display', 'none');
        }
        if (IsExemptCAF === "Y") {
            $("#IsExemptCaf").css('display', 'flex');

        }

    }
