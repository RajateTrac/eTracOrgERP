@model WorkOrderEMS.Models.BenefitList


<div style="margin: 83px; padding:0px 0 0 44px">
    <div class="form-group row" style="text-align: center;padding: 36px;background: darkblue;">
        <div class="col-sm-3">
            <img src="~/Images/floridablue.png" style="width: 155px;" />
        </div>
        <div class="col-sm-6">
            <label style="font-size: 45px;color: #3c94e0;font-weight: 500;">Benefits</label>
        </div>
        <div class="col-sm-3">
            <img src="~/Images/floridablue.png" style="width: 155px;" />
        </div>
    </div>
    <form id="SaveBenifit">

        <div class="form-group row">
            @if (Model != null)
            {
                if (Model.configurations != null)
                {
                    foreach (var item in Model.configurations[0].elements.serviceType.values)
                    {
                        <div class="col-sm-3" style="padding:7px;">
                            <input type="checkbox" value="@item.code" />&nbsp;&nbsp;&nbsp;&nbsp;@item.value<br />
                        </div>
                    }
                }
                else
                {
                    <div style="color:black;">No Data found...! please continue your onboarding process.</div>
                }
            }

        </div>

        @*<input type="checkbox" />Benifit 1<br /><br />
            <input type="checkbox" />Benifit 2<br /><br />
            <input type="checkbox" />Benifit 3<br /><br />
            <input type="checkbox" />Benifit 4<br /><br />
            <input type="checkbox" />Benifit 5<br /><br />
            <input type="checkbox" />Benifit 6<br /><br />*@
    </form>
    <span style="cursor:pointer;" onclick="SubmitForm(this, SaveBenifit)" title="Submit"><a><i class="fa fa-long-arrow-right" style="font-size: 40px;color: steelblue;margin-left: 88%;"></i></a></span>
</div>

