                <div class="col-lg-3"></div>
                <div class="col-lg-9 col-md-12">
                    <p style="font-size:18px;">Name of High School @Html.TextBoxFor(x => item.HighSchool.SchoolName, new { @class = "text-line", @style = "width:76.5%;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.Name, "", new { @style = "color:red;" })</p>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3"></div>
                <div class="col-lg-9 col-md-12">
                    <p style="font-size:18px;">
                        City @Html.TextBoxFor(x => item.HighSchool.City, new { @class = "text-line", @style = "width:46%;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.HighSchool.City, "", new { @style = "color:red;" })
                        State @Html.TextBoxFor(x => item.HighSchool.State, new { @class = "text-line", @style = "width:345px;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.HighSchool.State, "", new { @style = "color:red;" })
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">

                    <p style="font-size:18px;">
                        Attended from @Html.TextBoxFor(x => item.HighSchool.AttendFrom, "{0:yyyy-MM-dd}", new { @class = "text-line date", @style = "width:35%;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.HighSchool.AttendFrom, "", new { @style = "color:red;" })
                        <br />(Please provide month and year)
                    </p>


                    <p style="font-size:18px;">
                        Attended to @Html.TextBoxFor(x => item.HighSchool.AttendTo, "{0:yyyy-MM-dd}", new { @class = "text-line", @style = "width:36.5%;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.HighSchool.AttendTo, "", new { @style = "color:red;" })
                    </p>

                </div>
            </div>
            if (item.HigherSchool != null && item.HigherSchool.Cretificate != null)
            {
                <div class="row">
                    <div class="col-lg-3"></div>
                    <div class="col-lg-9 col-md-12">
                        <p style="font-size:18px;">
                            Type of Diploma/Certificate @Html.TextBoxFor(x => item.HigherSchool.Cretificate, new { @class = "text-line", @style = "width:71.5%;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" }) @Html.ValidationMessageFor(x => item.HigherSchool.AttendFrom, "", new { @style = "color:red;" })
                            <br />(Highest Degree Obtained)
                        </p>
                    </div>
                </div>
            }
        }
        <div class="row">
            <div class="col-lg-3"></div>
            <div class="col-lg-9 col-md-12">
                <p style="font-size:18px;">
                    I authorize AllStar Backgrounds to conduct an investigation of my education history and further release the companies, schools, or persons from liability or damages for providing information.<br />
                    I hereby state that the information on this form is true and complete. In the event of employment, I understand that false, misleading or omitted information given in application
                    documents or employment interviews may result in dismissal. I agree that the above information is true.
                </p>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-3"></div>
            <div class="col-lg-9 col-md-12">
                <span>
                    <span id="ShowHideSignatureSpan">
                        <input type="text" style="width:290px;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" class="text-line" />
                    </span>
                    <img id="openItForSignature" style="display:none;width:163px;height:47px;" />
                    <span>
                        @*<span class="btn btn-success" style="border-radius:100px;width:20%;" id="AddSignature">
                                Add Signature
                            </span>*@
                    </span><br />Signature
                </span>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-3"></div>
            <div class="col-lg-9 col-md-12">
                <p><input type="text" style="width:290px;background-color: transparent;outline: none;outline-style: none;border-top: none; border-left: none;border-right: none;border-bottom: solid #000 1px;" id="ShowHideSignatureSpan" class="text-line" /><br />Date</p>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-3"></div>
            <div class="col-lg-9 col-md-12">
                <p><b><center>AllStar Backgrounds – <a href="mailto: info@allstarbackgrounds.com">info@allstarbackgrounds.com</a> – Ph 800-570-0546 Fax 888-502-1299</center></b></p>



            </div>
        </div>
    </div>
    <br>
    <br>
    <br>
    <br>

}


